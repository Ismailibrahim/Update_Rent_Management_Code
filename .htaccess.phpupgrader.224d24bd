# Use PHP56
#AddHandler application/x-httpd-php56 .php

RewriteEngine On
RewriteCond %{HTTPS} !=on
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
#maaidh
RewriteRule ^inventory/? https://hospitalitytechnology.com.mv/inventory [R=301,L]
RewriteRule ^support/? https://support.hospitalitytechnology.com.mv/ [R=301,L]



RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.+)$ $1.php [L]

options -Indexes 

