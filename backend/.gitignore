# Laravel Backend .gitignore

# Laravel specific
/vendor/
/node_modules/
/public/build/
/public/hot
/public/storage
/storage/*.key
/storage/pail
/storage/logs/*.log
/storage/framework/cache/data/*
/storage/framework/sessions/*
/storage/framework/testing/*
/storage/framework/views/*
/storage/app/public/*
!/storage/app/public/.gitkeep
!/storage/framework/cache/.gitkeep
!/storage/framework/sessions/.gitkeep
!/storage/framework/testing/.gitkeep
!/storage/framework/views/.gitkeep

# Environment files
.env
.env.backup
.env.production
.env.local
.env.*.local

# Logs
*.log
laravel.log

# IDE files
.phpactor.json
.phpunit.result.cache
/.fleet
/.idea
/.nova
/.phpunit.cache
/.vscode
/.zed

# Authentication
/auth.json

# Homestead
Homestead.json
Homestead.yaml
Vagrantfile

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
Desktop.ini

# PHP specific
composer.phar
composer.lock

# Testing
/coverage/
/.phpunit.cache/
phpunit.xml

# Temporary files
*.tmp
*.temp
*.swp
*.swo
*~

# Backup files
*.bak
*.backup
*.old

# Archive files
*.zip
*.tar.gz
*.rar

# Database files (if using SQLite for testing)
*.sqlite
*.sqlite3
*.db

# Cache files
bootstrap/cache/*.php
bootstrap/cache/packages.php
bootstrap/cache/services.php
bootstrap/cache/routes-v7.php

# Compiled assets
/public/mix-manifest.json
/public/js/
/public/css/

# Node.js (if using Laravel Mix)
/node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Editor directories and files
.vscode/*
!.vscode/extensions.json
.idea
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# Local development
.local